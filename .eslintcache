setPageNum={setPageNum}\n        totalPageNum={totalPageNum}\n      />\n      <Footer />\n    </div>\n  );\n};\n\nexport default TopRatedPage;\n","/Users/thienminh/Documents/coderschool/films/src/components/Cards.js",["77","78","79"],"import React, { useEffect, useState } from \"react\";\nimport { Card } from \"react-bootstrap\";\nimport { useHistory } from \"react-router-dom\";\n\nconst Cards = ({ movies }) => {\n  const history = useHistory();\n  const handleClickPhim = (phimId) => {\n    history.push(`/phim/${phimId}`);\n  };\n\n  return (\n    <div className=\"wrapper\" hover>\n      {movies &&\n        movies.map((film) => (\n          <div\n            className=\"card mt-4 ml-4\"\n            overlay=\"red-strong\"\n            style={{ width: \"13rem\", border: \"none\" }}\n            onClick={() => handleClickPhim(film.id)}\n          >\n            <img\n              src={`https://image.tmdb.org/t/p/w400/${film.poster_path}`}\n              alt=\"card img\"\n            />\n            <div className=\"info\">\n              <h3>{film?.title}</h3>\n              <p>\n                {film?.overview?.length < 90\n                  ? film.overview\n                  : film.overview.slice(0, 75) + \"...\"}\n              </p>\n              <button> Read More</button>\n            </div>\n          </div>\n        ))}\n    </div>\n  );\n};\n\nexport default Cards;\n","/Users/thienminh/Documents/coderschool/films/src/components/PaginationBar.js",[],"/Users/thienminh/Documents/coderschool/films/src/components/PublicNavbar.js",["80","81"],"import React from \"react\";\nimport { Navbar, Nav, NavDropdown } from \"react-bootstrap\";\nimport logo from \"../images/logo.png\";\nimport { FontAwesomeIcon } from \"@fortawesome/react-fontawesome\";\nimport { faSearch } from \"@fortawesome/free-solid-svg-icons\";\n\nimport { NavLink } from \"react-router-dom\";\nimport SearchForm from \"./SearchForm\";\n\nconst PublicNavbar = ({\n  loading,\n  searchInput,\n  handleSearc